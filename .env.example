# Ejemplo de archivo .env
# Copia este archivo como .env y completa con tus valores reales

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# API Configuration
EXPO_PUBLIC_API_BASE_URL=https://server-agricultura-db.onrender.com
EXPO_PUBLIC_DIAGNOSIS_ENDPOINT=/diagnosis
EXPO_PUBLIC_QUESTION_ENDPOINT=/question

# App Configuration
EXPO_PUBLIC_APP_NAME=forestAppPhone
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_APP_ENVIRONMENT=development

# Debug Configuration
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=info
